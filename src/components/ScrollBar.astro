<script>
gsap.registerPlugin(ScrollTrigger);

gsap.to(".scrollbar", {
    scrollTrigger: {
        trigger: ".scrollbar",
        start: "top center",
        end: "bottom",
        scrub: 1,
        markers: true,
        toggleActions: "restart none none none"
    },
    x: 400,
    rotation: 360,
    duration: 3
});
</script>